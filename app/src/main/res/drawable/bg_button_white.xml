<?xml version="1.0" encoding="utf-8"?>
<!-- https://developers.google.com/identity/branding-guidelines#sign-in-button -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="false"
        android:drawable="@color/bg_button_white_disabled" />
    <item
        android:state_pressed="true"
        android:drawable="@color/bg_button_white_pressed" />
    <item
        android:state_focused="true"
        android:drawable="@color/bg_button_white_focused" />
    <item
        android:drawable="@color/bg_button_white_normal" />
</selector>
